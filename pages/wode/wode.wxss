/* pages/wode/wode.wxss */
.wode_list{
  width: 100%;
  height: 100%;
  background: #fff;
}
.logo{
  display: block;
  width: 132rpx;
  height: 132rpx;
  margin: 0 auto;
  margin-top: 60rpx;
  border-radius: 50%;
  margin-bottom: 20rpx;
}
.logo_text{
  display: block;
  text-align: center;
  font-size: 32rpx;
  color: #333333;
  letter-spacing: 1.67rpx;
}
.wode_btn{
  width: 260rpx;
  height: 60rpx;
  text-align: center;
  line-height: 60rpx;
  font-size: 26rpx;
  color: #FFFFFF;
  background: #799DF1;
  border-radius: 8rpx;
  margin: 0 auto;
  margin-top: 40rpx;
}
.wode_btn3{
  width: 260rpx;
  height: 50rpx;
  text-align: center;
  line-height: 50rpx;
  font-size: 26rpx;
  color: #FFFFFF;
  background: #ccc;
  border-radius: 8rpx;
  margin: 0 auto;
  margin-top: 50rpx;
}
.wode_bor{
  width: 100%;
  height: 20rpx;
  background: #EBEBEB;
  margin-top: 30rpx;
  margin-bottom: 20rpx;
}
.list{
  position: relative;
}
.list view{
  width: 670rpx;
  height: 115rpx;
  border-bottom: 1rpx solid #CCCCCC;
  margin: 0 auto;
}
.list image{
  float: left;
  width: 40rpx;
  height: 40rpx;
  margin-right: 34rpx;
  margin-top: 42rpx;
}
.list text{
  float: left;
  font-size: 32rpx;
  color: #333333;
  letter-spacing: 1.67rpx;
   margin-top: 40rpx;
}
.fenxiang{
  position: absolute;
  width: 100%;
  left: 0;
  top: 0;
  height: 115rpx;
  opacity: 0;
}
.shibai_tishi{
  font-size: 28rpx;
  color: #E2442D;
  margin-top: 100rpx;
  line-height: 34rpx;
  display:flex;
  align-items: center;
  justify-content: center;
}
.shibai_tishi image{
  width: 34rpx;
  height: 34rpx;
  margin-right: 20rpx;
}
